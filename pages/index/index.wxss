.swiper-container {
  width: 750rpx;
  background: #fff;
  position: relative;
}

.swiper_box {
  width: 750rpx;
  height: 400rpx;
}

swiper-item image {
  width: 750rpx;
  height: 100%;
  display: inline-block;
  overflow: hidden;
}

.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 30rpx;
  display: flex;
  justify-content: center;
  z-index: 100;
}

.swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 50%;
  transition: all 0.6s;
  opacity: 0.5;
}

.swiper-container .dots .dot.active {
  width: 14rpx;
  opacity: 1;
}

.navigator-box {
  background: #fff;
  width: 100%;
  padding: 40rpx 0;
  box-sizing: border-box;
}

.navigator-item {
  width:33.3%;
  display:flex;
  float:left;
}

.navigator-item .item {
  width: 100%;
  height: 200rpx;
  padding-top: 35rpx;
  text-align: center;
}

.navigator-item .item .img {
  width: 100%;
  height: 100rpx;
  margin-bottom: 25rpx;
}

.navigator-item .item image {
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
}